{
  "rules": {
    "newline-per-chained-call": 0,
    "quotes": 0,
    "no-return-await": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "comma-dangle": [
      1, "always-multiline"
    ],
    "no-underscore-dangle": [
      0
    ]
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",

}
